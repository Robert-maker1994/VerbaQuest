{
	"$schema": "https://biomejs.dev/schemas/json/schema.json",
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true,
		
		"suspicious": {
		  "noExplicitAny": "warn",
		  "noConsoleLog": "warn"
		},
		"correctness": {
		  "noUnusedVariables": "warn",
		  "noUnreachable": "warn"
		}
	  }
	},
	"formatter": {
	  "enabled": true,
	  "lineWidth": 120,
	  "indentStyle": "space",
	  "indentWidth": 2
	},
	"javascript": {
	  "formatter": {
		"quoteStyle": "double",
		"semicolons": "always",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"jsxQuoteStyle": "double",
		"quoteProperties": "asNeeded",
		"trailingCommas": "all"
		 
	  }
	}
  }